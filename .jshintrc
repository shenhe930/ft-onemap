{

    // Enforcing options
    // http://www.jshint.com/docs/options/#enforcing-options

    "bitwise": true,
    "eqeqeq": true,
    "forin": true,
    "latedef": true,
    "noarg": true,
    "nonbsp": true,
    "nonew": true,
    "undef": true,
    "unused": true,

    // - - - - - - - - - - - - - - - - - - - - - - - - - - -

    // Relaxing options
    // http://www.jshint.com/docs/options/#relaxing-options

    "esnext": true,

    // - - - - - - - - - - - - - - - - - - - - - - - - - - -

    // Environments
    // http://www.jshint.com/docs/options/#environments

    "browser": true,
    "jquery": true,
    "node": true,

    "predef": ["BMap",
        "BMapLib",
        "BMAP_ANCHOR_TOP_LEFT",
        "BMAP_ANCHOR_TOP_RIGHT",
        "BMAP_ANCHOR_BOTTOM_RIGHT",
        "BMAP_NAVIGATION_CONTROL_ZOOM",
        "layer",
        "laydate",
        "alert"
    ]
}
